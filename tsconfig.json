{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "verbatimModuleSyntax": true,
        "moduleResolution": "bundler",
        "paths": {
            "@/*": ["./*"]
        },
        "types": ["uniwind/types"]
    },
    "include": ["**/*.ts", "**/*.tsx"]
}